<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
    <Button name="BFI_ObjectiveTrackerPOIButtonTemplate" inherits="POIButtonTemplate" virtual="true">
        <Animations>
            <AnimationGroup parentKey="AddAnim" setToFinalAlpha="true">
                <Scale duration="0.15" order="1" fromScaleX="1" fromScaleY="1" toScaleX="1.2" toScaleY="1.2"/>
                <Scale duration="0.15" order="1" startDelay="0.15" fromScaleX="1.2" fromScaleY="1.2" toScaleX="1" toScaleY="1"/>
                <Alpha duration="0.15" order="1" fromAlpha="0" toAlpha="1"/>
            </AnimationGroup>
        </Animations>
    </Button>

    <Frame name="BFI_ObjectiveTrackerQuestPOIBlockTemplate" inherits="BFI_ObjectiveTrackerAnimBlockTemplate" mixin="BFI_ObjectiveTrackerQuestPOIBlockMixin" hidden="true" virtual="true"/>
</Ui>